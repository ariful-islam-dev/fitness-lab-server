{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "price": {
      "type": "string"
    },
    "discount_price": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "thumbnails": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "stock": {
      "type": "string"
    },
    "model": {
      "type": "string"
    },
    "material": {
      "type": "string"
    },
    "tags": {
      "type": "string"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "product"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "warranty": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::warranty.warranty",
      "inversedBy": "products"
    },
    "metarial": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::metarial.metarial",
      "inversedBy": "products"
    },
    "colors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::color.color",
      "mappedBy": "product"
    },
    "sizes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::size.size",
      "mappedBy": "product"
    }
  }
}
